<p id="notice"><%= notice %></p>

<h1>Listing Subestacions</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subestacions.each do |subestacion| %>
      <tr>
        <td><%= subestacion.codigo %></td>
        <td><%= subestacion.nombre %></td>
        <td><%= link_to 'Show', subestacion %></td>
        <td><%= link_to 'Edit', edit_subestacion_path(subestacion) %></td>
        <td><%= link_to 'Destroy', subestacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subestacion', new_subestacion_path %>
