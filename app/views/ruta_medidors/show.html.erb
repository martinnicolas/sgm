<div class="page-header">
  <%= link_to ruta_medidors_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Volver
  <% end %>
  <%= link_to edit_ruta_medidor_path(@ruta_medidor), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Editar
  <% end %>
  <h4>Ver ruta_medidor</h4>
</div>

<dl class="dl-horizontal">
  <dt>Ruta:</dt>
  <dd><%= @ruta_medidor.rutum %></dd>

  <dt>Medidor:</dt>
  <dd><%= @ruta_medidor.medidor %></dd>

</dl>
